    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-winston": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.40",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.26.0",
    "fetch-mock": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
